<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dong.cbu.dao.OrderTableMapper">
    <resultMap id="BaseResultMap" type="com.dong.cbu.model.OrderTable">
        <id column="id" property="id" jdbcType="BIGINT"/>
        <result column="planId" property="planId" jdbcType="BIGINT"/>
        <result column="memberId" property="memberId" jdbcType="BIGINT"/>
        <result column="seatId" property="seatId" jdbcType="BIGINT"/>
        <result column="money" property="money" jdbcType="BIGINT"/>
        <result column="state" property="state" jdbcType="BIGINT"/>
        <result column="created" property="created" jdbcType="TIMESTAMP"/>
    </resultMap>
    <insert id="order" parameterType="com.dong.cbu.model.OrderTable">
        insert INTO ordertable (id,planId,memberId,seatId,money,state,created)
        VALUES
        (#{id,jdbcType=BIGINT},#{planId,jdbcType=BIGINT},#{memberId,jdbcType=BIGINT},#{seatId,jdbctype=BIGINT},#{money,jdbctype=BIGINT},
        #{state,jdbcType=BIGINT},#{created,jdbctype=TIMESTAMP})
    </insert>

    <select id="showCheck" parameterType="com.dong.cbu.model.OrderTable">
        SELECT <include refid="BaseResultMap"/> FROM ordertable WHERE id = #{memberId,jdbcType=BIGINT};
    </select>
</mapper>